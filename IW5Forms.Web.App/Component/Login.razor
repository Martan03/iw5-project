@using Microsoft.AspNetCore.Components.Authorization
@using Microsoft.AspNetCore.Components.WebAssembly.Authentication
@inject NavigationManager Navigation
@namespace IW5Forms.Web.App


<AuthorizeView>
    <Authorized>
        <MudIconButton Href="authentication/logout" Icon="@Icons.Material.Outlined.Logout" Variant="Variant.Outlined" Color="Color.Primary" Size="Size.Medium" />
    </Authorized>
    <NotAuthorized>
        <MudIconButton Href="authentication/login" Icon="@Icons.Material.Outlined.Person" Variant="Variant.Outlined" Color="Color.Primary" Size="Size.Medium" />
    </NotAuthorized>
</AuthorizeView>
